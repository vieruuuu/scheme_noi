{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".nim",
                "isAsync": true,
                "cmd": "nimpretty --indent:2 ${file}"
            }
        ]
    },
    "files.associations": {
        "string": "cpp",
        "*.txt": "cpp"
    },
    "cSpell.words": [
        "HBITMAP",
        "Kometa",
        "Orbitum",
        "Yandex",
        "booldefine",
        "pecxstudios"
    ]
}