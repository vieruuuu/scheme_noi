        "PVOID",
        "SSID",
        "UHDA",
        "Uninitialize",
        "Yandex",
        "asdu",
        "booldefine",
        "karax",
        "karaxdsl",
        "pcname",
        "pecxstudios",
        "pptx",
        "pwsz",
        "tdiv",
        "usbs",
        "vdom",
        "vnode",
        "winmgmts",
        "wmploc",
        "xxtea"
