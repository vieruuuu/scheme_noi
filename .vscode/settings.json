{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".nim",
                "isAsync": true,
                "cmd": "nimpretty --indent:2 ${file}"
            }
        ]
    },
    "files.associations": {
        "string": "cpp",
        "*.txt": "cpp"
    },
    "cSpell.words": [
        "CLSCTX",
        "CLSID",
        "HBITMAP",
        "Kometa",
        "LPCWSTR",
        "Orbitum",
        "PULARGE",
        "PVOID",
        "Uninitialize",
        "Yandex",
        "booldefine",
        "pecxstudios",
        "pptx",
        "wmploc",
        "xxtea"
    ]
}